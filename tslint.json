{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "semicolon": [true, "never"],
    "no-console": [false, "log", "error"],
    "indent": [true, "spaces", 2],
    "trailing-comma": [true, { "multiline": "never", "singleline": "never" }]
  },
  "rulesDirectory": []
}
